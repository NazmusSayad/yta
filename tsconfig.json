{
  "compilerOptions": {
    "moduleResolution": "nodenext",
    "target": "es6",
    "skipLibCheck": true,
    "declaration": true,
    "inlineSourceMap": false,
    "strict": true,
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true
  },
  "include": ["src"]
}
